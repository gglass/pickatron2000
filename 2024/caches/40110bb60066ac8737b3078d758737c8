    else {
        console.log('push ad:div-gpt-ad-728x90-Footer');
        freestar.config.enabled_slots.push({ placementName: "div-gpt-ad-728x90-Footer", slotId: "div-gpt-ad-728x90-Footer" });
    }
  </script>
</div>
<!-- /div.#fs_fs_footer -->

</div>





