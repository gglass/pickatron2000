      <tr class="over_header"><th></th><th></th><th></th>
         <th aria-label="" data-stat="header_tot_off_tgl" colspan="3" class=" over_header center" >Tot Yds &amp; TO</th><th></th><th></th>
         <th aria-label="" data-stat="header_pass" colspan="7" class=" over_header center" >Passing</th>
         <th aria-label="" data-stat="header_rush" colspan="5" class=" over_header center" >Rushing</th>
         <th aria-label="" data-stat="header_penalties" colspan="3" class=" over_header center" >Penalties</th>
         <th aria-label="" data-stat="" colspan="3" class=" over_header center" ></th>
         <th aria-label="" data-stat="averages" colspan="5" class=" over_header center" >Average Drive</th>
      </tr>
            
      <tr>
         <th aria-label="" data-stat="player" scope="col" class=" poptip sort_default_asc show_partial_when_sorting left" >Player</th>
         <th aria-label="Points For" data-stat="points" scope="col" class=" poptip center" data-tip="Points Scored by team" data-over-header="Points" >PF</th>
         <th aria-label="Yds" data-stat="total_yards" scope="col" class=" poptip center" >Yds</th>
         <th aria-label=" Offensive Plays Run " data-stat="plays_offense" scope="col" class=" poptip center" data-tip="Offensive Plays: Pass Attempts + Rush Attempts + Times Sacked" data-over-header="Tot Yds &amp; TO" >Ply</th>
         <th aria-label=" Yards Per Offensive Play " data-stat="yds_per_play_offense" scope="col" class=" poptip center" data-tip="Yards per Offensive Play<br>(Rush + Pass Yards)/( Pass Attempts + Rush Attempts + Times Sacked)" data-over-header="Tot Yds &amp; TO" >Y/P</th>
         <th aria-label="Turnovers" data-stat="turnovers" scope="col" class=" poptip center" data-tip="Team Turnovers Lost" data-over-header="Tot Yds &amp; TO" >TO</th>
